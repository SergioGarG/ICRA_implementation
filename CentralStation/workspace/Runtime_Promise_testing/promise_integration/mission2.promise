mission{         
	conditions{     
		events:
			loading: "loading",
			unloading: "unloading"		
		actions:
			a1: "col_loading",
			a2: "col_unloading"
			}  
	robots tiago 
	locations: r1, r2, r3 , r4  
	operators { parallel ( 
		eventHandler ( 
			default ( delegate ( robot tiago does Patrolling locations r3 , r4 ) ) 
			except loading (  delegate ( robot  tiago does SimpleAction actions a1 affecting event loading ) )
			except unloading ( delegate ( robot tiago does SimpleAction actions a2 affecting event unloading ) ) )
		
		 )
	}    
}              
  
             