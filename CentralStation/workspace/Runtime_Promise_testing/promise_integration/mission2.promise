mission{ 
	conditions{ 
		events
 		e1: "description of event e1"
		actions
 		a1: "description of action a1"

	} 
	robots r1, r2
	locations l1, l2, l3, l4
	operators{ parallel(  
		delegate (StrictOrderedPatrolling locations l1 ,  l3),  
		eventHandler ( 
			default ( delegate (Patrolling locations l1 , l3 )) 
			except e1 ( delegate (Visit locations l2 , l4 affecting event e1 ) ) ) )
	} 
}       