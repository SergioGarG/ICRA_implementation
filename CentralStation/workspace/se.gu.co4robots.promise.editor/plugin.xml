<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated promise -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="promise.presentation.PromiseModelWizardID"
            name="%_UI_PromiseModelWizard_label"
            class="promise.presentation.PromiseModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/PromiseModelFile.gif">
         <description>%_UI_PromiseModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated promise -->
      <editor
            id="promise.presentation.PromiseEditorID"
            name="%_UI_PromiseEditor_label"
            icon="icons/full/obj16/PromiseModelFile.gif"
            extensions="promise"
            class="promise.presentation.PromiseEditor"
            contributorClass="promise.presentation.PromiseActionBarContributor">
      </editor>
   </extension>

</plugin>
