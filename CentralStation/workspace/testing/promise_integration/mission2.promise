mission{ 
	conditions{ 
		events
 		e1: "description of event e1"
		actions
 		a1: "description of action a1"

	} 
	robots r1
	locations l1, l2, l3
	operators{ parallel{ 
		// Your operators come here
		r1 ( eventHandler ( 
			default ( 
				delegate ( Patrolling locations l1 , l2 )
		) 
			except e1 ( 
				delegate ( SimpleAction actions a1 )
			) ) ) }
	}
}